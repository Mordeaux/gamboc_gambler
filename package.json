{
  "name": "gomboc_gambler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate-seed && next dev",
    "build": "next build",
    "start": "npm run migrate-seed && next build && next start",
    "lint": "next lint",
    "seed": "prisma db seed",
    "migrate": "prisma db push",
    "migrate-seed": "npm run migrate && npm run seed",
    "test": "DATABASE_URL=\"file:./gambler-test.db\" npm run migrate-seed && jest"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.40",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}